<input v-model=\"formData.audience\" type=\"text\">\n          </label>\n          \n          <label>\n            输出要求：\n            <input v-model=\"formData.output\" type=\"text\">\n          </label>\n          <!-- 增加其他表单字段 -->\n          <button type=\"submit\">提交</button>\n        </form>\n    </div>\n\n  <div>{{ message }}</div>\n  <button @click=\"handleClick\">点击</button>\n\n</template>\n\n<script>\nimport axios from 'axios'; // 引入axios库\n\nexport default {\n  data() {\n    return {\n      message: '', // 定义message变量，用于存储api返回的数据\n      formData:{\n          context:'',\n          goal:'',\n          style:'',\n          tone:'',\n          audience:'',\n          output:''\n          }\n    }\n  },\n  components:{\n      },\n\n  methods: {\n     handleClick() {\n       axios.get('http://localhost:8002/api/response/?format=json')  //你的Django后端API的endpoint\n          .then(response => {\n             //在这里处理你的数据。response.data将包含你的数据。\n             console.log(response.data);\n             this.message = response.data.message;\n          })\n          .catch(error => {\n             //在这里处理错误\n             console.error(error);\n          }); \n        },\n\n    submitForm() {\n      axios.post('http://localhost:8002/api/response/', this.formData)\n          .then(response => {\n            // 处理你后端返回的response\n            console.log(response);\n            this.message = response.data.message;\n        })\n        .catch(err => {\n          console.log(err);\n        });\n    }\n   },\n\n  mounted() {\n  }\n}\n</script>\n"],"mappings":"AA4CA,OAAOA,KAAI,MAAO,OAAO,EAAE;;AAE3B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MAAE;MACbC,QAAQ,EAAC;QACLC,OAAO,EAAC,EAAE;QACVC,IAAI,EAAC,EAAE;QACPC,KAAK,EAAC,EAAE;QACRC,IAAI,EAAC,EAAE;QACPC,QAAQ,EAAC,EAAE;QACXC,MAAM,EAAC;MACP;IACN;EACF,CAAC;EACDC,UAAU,EAAC,CACP,CAAC;EAELC,OAAO,EAAE;IACNC,WAAWA,CAAA,EAAG;MACZZ,KAAK,CAACa,GAAG,CAAC,iDAAiD,EAAG;MAAA,CAC1DC,IAAI,CAACC,QAAO,IAAK;QACf;QACAC,OAAO,CAACC,GAAG,CAACF